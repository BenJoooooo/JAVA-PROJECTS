import java.util.Scanner;
public class Main {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Scanner in = new Scanner (System.in);
		int total = 0;
		int grades;
		int average;
		int counter = 0;
		
		while (counter < 5) {
			grades = in.nextInt();
			total = total + grades;
			counter++;
		}
		average = total / 5;
		System.out.print("Average = " + average);
	}

}
