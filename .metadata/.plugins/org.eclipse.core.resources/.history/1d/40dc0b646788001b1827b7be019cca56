package com.ben.codingPractice;

abstract class Dog {
	String breed;
	public void bark() {
		System.out.println("Bark!");
	}
}

class Chihuahua extends Dog {
	
}
public class Abstraction {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Chihuahua d = new Chihuahua();
		d.bark();
	}

}
