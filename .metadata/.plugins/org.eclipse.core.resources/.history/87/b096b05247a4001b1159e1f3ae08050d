
public class PersonClass {
	
	private String name;
	private int age;
	
	public PersonClass() {
		name = "Default Name";
		age = 0;
	}
	
//	Getters Method
	public String getName() {
		return name;
	}
	public int getAge() {
		return age;
	}
	
//	Setters Method
	public void setName(String name) {
		this.name = name;
	}
	public void setAge(int age) {
		this.age = age;
	}
	
}
